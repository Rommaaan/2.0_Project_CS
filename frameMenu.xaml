<Page x:Class="LoginISP.frameMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LoginISP"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="frameMenu">

    <Grid>
        <TextBlock Text="Страница Меню" x:Name="txblTextBlock" FontFamily="Candara" FontSize="16"/>
        <StackPanel Orientation="Vertical" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="btnButton" x:FieldModifier="public" Content="Login" Width="100" Height="20" Background="White" Click="btnButton_Click"/>
        </StackPanel>
    </Grid>
</Page>
